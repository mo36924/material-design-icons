{
  "compilerOptions": {
    "noEmit": true,
    "target": "ES2020",
    "module": "ESNext",
    "allowJs": true,
    "declaration": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "importsNotUsedAsValues": "error"
  }
}
